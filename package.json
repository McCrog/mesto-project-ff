{
  "name": "mesto-project",
  "version": "1.0.0",
  "description": "https://github.com/McCrog/mesto-project-ff",
  "main": "index.js",
  "scripts": {
    "check": "prettier --check './**/*.{html,css,js}'",
    "format": "prettier --write './**/*.{html,css,js}'",
    "lint": "stylelint '**/*.css'",
    "lintfix": "stylelint '**/*.css' --fix",
    "checkAll": "npm run check && npm run lint",
    "fixAll": "npm run format && npm run lintfix",
    "fixAndUpdate": "npm run fixAll && git add . && git commit --amend --no-edit",
    "fixAndAdd": "npm run fixAll && git add . && git commit -m 'CHANGE'",
    "fixAndPush": "npm run fixAll && git add . && git commit -m 'prepared for publication' && git push",
    "build": "webpack --mode production",
    "dev": "webpack serve",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "buildTs": "tsc src/indexTs.ts --declaration",
    "buildTs:dev": "tsc src/indexTs.ts --sourceMap",
    "watchTs": "tsc src/indexTs.ts --watch",
    "lintTs": "eslint src",
    "fixTs": "eslint --fix src"
  },
  "author": "McCrog",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "autoprefixer": "^10.4.16",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-promise": "^6.1.1",
    "gh-pages": "^6.1.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.3",
    "stylelint": "^15.10.3",
    "stylelint-order": "^6.0.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "core-js": "^3.33.3"
  }
}
